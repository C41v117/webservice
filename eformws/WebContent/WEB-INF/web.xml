<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.4">
  <display-name>eform-ws</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>mvc-dispatcher</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>mvc-dispatcher</servlet-name>
    <url-pattern>/rest/*</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>
			/WEB-INF/connection.xml,
			/WEB-INF/service-applicationContext.xml,
			/WEB-INF/security-context.xml
		</param-value>
  </context-param>
  <filter>
    <filter-name>springSecurityFilterChain</filter-name>
    <filter-class>org.springframework.web.filter.DelegatingFilterProxy</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>springSecurityFilterChain</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener
		</listener-class>
  </listener>
  <listener>
    <listener-class>com.metamorf.eform.web.util.RSCFParameterLoader
		</listener-class>
  </listener>
  <servlet>
    <servlet-name>web-service</servlet-name>
    <servlet-class>
			com.sun.xml.ws.transport.http.servlet.WSSpringServlet
		</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>web-service</servlet-name>
    <url-pattern>/services/*</url-pattern>
  </servlet-mapping>
  <error-page>
    <exception-type>java.lang.Throwable</exception-type>
    <location>/genericerror.jsp</location>
  </error-page>
  <error-page>
    <error-code>400</error-code>
    <location>/error400.jsp</location>
  </error-page>
  <error-page>
    <error-code>401</error-code>
    <location>/error401.jsp</location>
  </error-page>
  <error-page>
    <error-code>403</error-code>
    <location>/error403.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/error404.jsp</location>
  </error-page>
  <error-page>
    <error-code>405</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>406</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>407</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>408</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>409</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>410</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>411</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>412</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>413</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>414</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>415</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>416</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>417</error-code>
    <location>/error40x.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/error500.jsp</location>
  </error-page>
  <error-page>
    <error-code>501</error-code>
    <location>/error50x.jsp</location>
  </error-page>
  <error-page>
    <error-code>502</error-code>
    <location>/error50x.jsp</location>
  </error-page>
  <error-page>
    <error-code>503</error-code>
    <location>/error503.jsp</location>
  </error-page>
  <error-page>
    <error-code>504</error-code>
    <location>/error50x.jsp</location>
  </error-page>
  <error-page>
    <error-code>505</error-code>
    <location>/error50x.jsp</location>
  </error-page>
  <error-page>
    <error-code>511</error-code>
    <location>/error50x.jsp</location>
  </error-page>
</web-app>